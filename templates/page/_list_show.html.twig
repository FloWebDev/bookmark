{% for list in page.listings %}
<div class="col-3">
    <div class="d-flex w-100 justify-content-between p-2 list-header">
        <h5 class="mb-1">{{ list.title }}</h5>
        <small><a href="" class="text-dark"><i class="fas fa-tools"></i></a></small>
    </div>
    <ul class="list-group">
        {% for item in list.items %}
        <li class="list-group-item d-flex justify-content-between align-items-center p-2">
            <a href="{{ item.url }}" target="_blank">{{ item.title }}</a>
            <span><a href="#"><i class="fas fa-arrow-up"></i></a> 
                <a href="#"><i class="fas fa-arrow-down"></i></a>
            </span>
        </li>
        {% else %}
        <li class="list-group-item d-flex justify-content-between align-items-center p-2">
            Aucun élément trouvé
        </li>
        {% endfor %}
    </ul>
</div>
{% else %}
    <p>Aucun élément trouvé</p>
{% endfor %}